{
  "name": "visual-host/payfast",
  "description": "Process online payments in Laravel with Payfast",
  "keywords": ["package", "laravel", "payfast"],
  "license": "MIT",
  "require": {
    "illuminate/support": "^6.0.2",
    "illuminate/http": "^6.0.2",
    "moneyphp/money": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit" : "4.*",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload": {
    "psr-4": {
      "VisualHost\\Payfast\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VisualHost\\Payfast\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
